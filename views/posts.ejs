<% include partials/header%>



<h1>The Posts Page</h1>


<% for(let i = 0; i < posts.length; i++) { %>
    <li>
        <%= posts[i].title %> - <b><%= posts[i].author %></b>
    </li>
<% } %>

<% posts.forEach(function(post) { %>
    <li>
            <%= post.title %> - <b><%= post.author %></b>
    </li>
<% }) %>

<% include partials/footer%>

